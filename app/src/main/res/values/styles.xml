<?xml version="1.0" encoding="utf-8"?>
<resources>
    <style name="CustomNoActionBarTheme" parent="@style/Theme.AppCompat.Light.NoActionBar">
        <item name="android:statusBarColor">@color/darkGreen</item>
    </style>
    <style name="CustomToolbarStyle">
        <item name="android:textColorPrimary">#757575</item>
        <item name="android:colorControlNormal">#757575</item>
    </style>
    <style name="AppTheme.Splash" parent="@style/Theme.AppCompat.NoActionBar">
        <item name="android:windowFullscreen">true</item>
        <item name="android:windowContentOverlay">@null</item>
    </style>
    <style name="HiddenTitleTheme">
        <item name="windowNoTitle">true</item>
        <item name="windowActionBar">false</item>
    </style>

    <style name="CustomNoActionBarThemeSplash" parent="@style/Theme.AppCompat.Light.NoActionBar">
        <item name="windowActionBar">false</item>
        <item name="windowNoTitle">true</item>
    </style>
</resources>